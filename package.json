{
  "name": "docker_express_app",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "start": "node index.js",
    "server": "npm run dev:tsc",
    "dev": "cross-env NODE_ENV=development vite --host=0.0.0.0",
    "dev:vite": "cross-env NODE_ENV=vite vite --host=0.0.0.0",
    "dev:tsc": "ts-node-dev -r tsconfig-paths/register --poll --transpile-only src/index.ts",
    "lint": "eslint --fix",
    "test": "cross-env NODE_ENV=testing vitest",
    "test:ui": "cross-env NODE_ENV=testing vitest --ui",
    "prepare": "husky install .husky",
    "format": "npx prettier --write ./src"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.2.1",
    "bcryptjs": "^2.4.3",
    "config": "^3.3.8",
    "connect-redis": "^6.1.3",
    "cors": "^2.8.5",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-session": "^1.17.3",
    "ioredis": "^5.2.5",
    "mongoose": "^6.7.2",
    "pino": "^8.7.0",
    "redis": "^4.5.1",
    "socket.io": "^4.5.4",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/connect-redis": "^0.0.19",
    "@types/cors": "^2.8.13",
    "@types/express-session": "^1.17.5",
    "@types/morgan": "^1.9.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vitest/ui": "^0.26.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "mongodb-memory-server": "^8.10.2",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "pretty-quick": "^3.1.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.9.3",
    "vite": "^4.0.2",
    "vite-plugin-live-reload": "^3.0.1",
    "vite-plugin-node": "^1.0.0",
    "vite-plugin-terminal": "^1.0.2",
    "vitest": "^0.26.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx pretty-quick --staged ng lint ng test",
      "pre-push": "ng build --aot true"
    }
  }
}
